; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="BENDYWORM"
run/main_scene="res://Main.tscn"
boot_splash/image="res://Textures/Logo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

LevelTilemaps="*res://Scenes/Singletons/LevelTilemaps.tscn"
PathRasterizer="*res://Scenes/Singletons/PathRasterizer.tscn"
Globals="*res://Scenes/Singletons/Globals.tscn"
FXSpawner="*res://Scenes/Singletons/FXSpawner.tscn"
UI="*res://Scenes/Singletons/UI.tscn"
Events="*res://Scenes/Singletons/Events.tscn"
Audio="*res://Scenes/Singletons/Audio.tscn"
SaveProgress="*res://Scenes/Singletons/SaveProgress.tscn"
Config="*res://Scenes/Singletons/Config.tscn"
Music="*res://Scenes/Singletons/Music.tscn"
MaterialCache="*res://Scenes/Singletons/MaterialCache.tscn"

[display]

window/size/width=1920
window/size/height=1080

[editor_plugins]

enabled=PoolStringArray( "res://addons/vnen.tiled_importer/plugin.cfg" )

[importer_defaults]

vnen.tiled_importer={
"add_background": false,
"collision_layer": 1,
"custom_properties": true,
"embed_internal_images": false,
"image_flags": 0,
"post_import_script": "",
"save_tiled_properties": true,
"tile_metadata": false,
"uv_clip": true
}
texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": true,
"flags/mipmaps": true,
"flags/repeat": true,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}
ogg_vorbis={
"loop": false,
"loop_offset": 0
}

